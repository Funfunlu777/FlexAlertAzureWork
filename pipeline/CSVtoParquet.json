{
	"name": "CSVtoParquet",
	"properties": {
		"activities": [
			{
				"name": "TransformRawFile",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings",
							"copyBehavior": "PreserveHierarchy"
						},
						"formatSettings": {
							"type": "ParquetWriteSettings"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "Date",
									"type": "DateTime",
									"physicalType": "String"
								},
								"sink": {
									"name": "Date",
									"type": "String",
									"physicalType": "UTF8"
								}
							},
							{
								"source": {
									"name": "Data type",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "Datatype",
									"physicalType": "UTF8"
								}
							},
							{
								"source": {
									"name": "Max Temperature",
									"type": "Double",
									"physicalType": "String"
								},
								"sink": {
									"name": "MaxTemperature",
									"physicalType": "UTF8"
								}
							},
							{
								"source": {
									"name": "Min Temperature",
									"type": "Double",
									"physicalType": "String"
								},
								"sink": {
									"name": "MinTemperature",
									"physicalType": "UTF8"
								}
							},
							{
								"source": {
									"name": "Precipitation",
									"type": "Double",
									"physicalType": "String"
								},
								"sink": {
									"name": "Precipitation",
									"type": "String",
									"physicalType": "UTF8"
								}
							},
							{
								"source": {
									"name": "Heat Stress",
									"type": "Int32",
									"physicalType": "String"
								},
								"sink": {
									"name": "HeatStress",
									"physicalType": "UTF8"
								}
							},
							{
								"source": {
									"name": "Cold Stress",
									"type": "Int32",
									"physicalType": "String"
								},
								"sink": {
									"name": "ColdStress",
									"physicalType": "UTF8"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "EOSWeatherCSV",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ParquetEOSWeather",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}